{
  "name": "sitespeedtools-www",
  "version": "1.0.0",
  "description": "Website generation code for SiteSpeedTools.com",
  "main": "index.js",
  "scripts": {
    "start": "parcel src/index.pug",
    "build": "parcel build src/index.pug --out-dir dist --public-url ./",
    "lint": "eslint 'src/**/*.js'",
    "lintfix": "eslint 'src/**/*.js --fix'"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^6.4.0",
    "@twind/core": "^1.1.3",
    "eslint": "^8.38.0",
    "parcel-bundler": "^1.12.5",
    "pug": "^3.0.2",
    "tailwindcss": "^3.3.1"
  },
  "browserslist": [
    "last 2 versions"
  ],
  "postcss": {
    "plugins": {
      "postcss-import": {},
      "postcss-nesting": {},
      "autoprefixer": {},
      "tailwindcss": {},
      "twind": {}
    }
  }
}
